{
  "name": "worldcup",
  "version": "1.0.0",
  "main": "index.js",
  "engine": "10.4.0",
  "license": "MIT",
  "scripts": {
    "clean:build": "rm -rf dist",
    "build": "yarn clean:build && yarn type && babel src -d dist --extensions '.ts,.tsx,.js,.jsx'",
    "dev": "tsc-watch --onSuccess \"yarn start:dev\"",
    "start:dev": "babel-node --extensions '.ts,.tsx,.js,.jsx' ./src/index.js",
    "start": "npm build && node dist/index.js",
    "type": "tsc",
    "test": "jest --config jest.config.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/node": "^7.0.0-beta.46",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-typescript": "^7.0.0-beta.46",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.8",
    "@types/request": "^2.47.0",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.6",
    "tsc-watch": "^1.0.21",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.49",
    "dotenv": "^6.0.0",
    "github-webhook-handler": "^0.7.1",
    "gmail-send": "^1.2.14",
    "moment": "^2.22.2",
    "ramda": "^0.25.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.2"
  }
}
